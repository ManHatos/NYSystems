// Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model records {
  log_id    String @id @db.VarChar(20) @unique
  staff_id  String @db.VarChar(20)
  roblox_id String @db.Text
  reason    String @db.VarChar(1000)
  action    Int    @db.TinyInt
  timestamp BigInt
}
